{
  "presets": ["@babel/preset-env"],
  "plugins": [
    ["@babel/plugin-transform-react-jsx", { "pragma": "jsx" }],
    "@babel/plugin-syntax-jsx"
  ],
  "env": {
    "test": {
      "plugins": [
        ["@babel/plugin-proposal-class-properties", { "loose": true }],
        ["@babel/plugin-transform-react-jsx", { "pragma": "jsx" }],
        "@babel/plugin-syntax-jsx"
      ]
    }
  }
}
