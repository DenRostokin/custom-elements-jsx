{
  "name": "custom-elements-jsx",
  "version": "1.0.6",
  "description": "JSX-render for custom elements",
  "main": "lib",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DenRostokin/custom-elements-jsx.git"
  },
  "scripts": {
    "build": "rimraf lib/* && babel src --out-dir lib",
    "livereload": "nodemon --ignore 'lib/' --exec npm run build",
    "prepublish": "npm run build",
    "test": "jest --colors",
    "test:watch": "jest --colors --watchAll",
    "test:coverage": "jest --colors --coverage"
  },
  "keywords": [
    "jsx",
    "render",
    "custom",
    "elements"
  ],
  "author": "Denis Rostokin",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-syntax-jsx": "^7.2.0",
    "@babel/plugin-transform-react-jsx": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^1.0.0",
    "eslint": "^5.11.1",
    "jest": "^23.6.0",
    "nodemon": "^1.18.9",
    "prettier-eslint": "^8.8.2",
    "rimraf": "^2.6.2"
  }
}
